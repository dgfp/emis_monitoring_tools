                        <select class="form-control input-sm" name="union" id="union" >
                            <option value="">- Select Union -</option>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-1 col-xs-2">
                        <label for="ward">Ward</label>
