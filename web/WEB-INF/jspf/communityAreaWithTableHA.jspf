                <div class="row">
                    <div class="col-md-1 col-xs-2">
                        <label for="reportType">Report Type</label>
                    </div>
                    <div class="col-md-2 col-xs-4" id="radioDiv">
                        <label><input type="radio" class="providerWise" id="reportType" name="reportType" value="aggregate" checked> Aggregate</label>
                        <label><input type="radio" class="overall" id="reportType" name="reportType" value="individual"> Individual</label>
